Host codereview.qt-project.org
    Port 29418

Host github.com
    Ciphers aes256-gcm@openssh.com,aes128-gcm@openssh.com,aes192-ctr,aes128-ctr
    ControlMaster no
    KexAlgorithms curve25519-sha256@libssh.org,diffie-hellman-group-exchange-sha256,diffie-hellman-group-exchange-sha1,diffie-hellman-group14-sha1
    MACs hmac-sha2-512

Host bitbucket.org
    ControlMaster no
    MACs hmac-sha1

Host ppc64
    Hostname aqua.opensuse.osuosl.org

Host review.source.android.com
    User cartman
    IdentityFile ~/.ssh/id_rsa

Host suse
    Hostname wotan.suse.de
    User idoenmez

Host susehome
    ProxyCommand ssh -q work nc -q0 wotan.suse.de 22
    User idoenmez

Host work
    Hostname i10z.com
    Port 45870

Host *.suse.de
    User idoenmez

Host *
    Ciphers chacha20-poly1305@openssh.com,aes256-gcm@openssh.com
    Compression yes
    ControlMaster auto
    ControlPath ~/.ssh/master-%r@%h:%p
    ControlPersist 24h
    HostKeyAlgorithms ssh-ed25519,ssh-rsa
    IdentityFile ~/.ssh/id_ed25519
    IdentityFile ~/.ssh/id_rsa
    KexAlgorithms curve25519-sha256@libssh.org,diffie-hellman-group-exchange-sha256
    MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com
    ServerAliveInterval 720
    ServerAliveCountMax 120
