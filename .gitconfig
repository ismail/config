[core]
	excludesfile = /home/ismail/.gitignore
	quotepath = false
	fileMode = false
[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
    graphviz = "!f() { echo 'digraph git {' ; git log --pretty='format:  %h -> { %p }' \"$@\" | sed 's/[0-9a-f][0-9a-f]*/\"&\"/g' ; echo '}'; }; f"
[user]
	name = İsmail Dönmez
	email = ismail@donmez.ws
[svn]
    brokenSymlinkWorkaround = false
[color]
	ui = auto
[sendemail]
	smtpserver = smtp.gmail.com
	smtpserverport = 587
	smtpencryption = tls
	smtpuser = ismail@donmez.ws
[url "git://anongit.kde.org/"]
     insteadOf = kde:
[url "git@git.kde.org:"]
     pushInsteadOf = kde:
[push]
	default = simple
